ALVWriteLog = function(str){
	var logfile = io.open("\alvtest.txt","a+");//打开文件
	var nowtime = time(); //创建时间对象实例
	nowtime.format="%m.%d %H:%M:%S";//设置时间格式
	logfile.write('\n'++tostring(nowtime)++str)//写入记录
	logfile.close()//关闭文件
}

