//调度函数
listener.Schedule = function(){
	var data = {0x20,0x03,0x22,0xf0,0x60,0xff,0xff,0xff};	
	for(i=1;8;1){
		var ret = LINDevMng.Transmit3C(data);
		if(#ret > 7){
			var baseline = (ret[4]<<8) + ret[5];
			var signal = (ret[6]<<8) + ret[7];
			thrdTable.lKeyData[i] = {"baseline"=baseline;"signal"=signal};
		}
		data[5] += 1;
		//sleep(1000);
	}
	data = {0x21,0x03,0x22,0xf0,0x60,0xff,0xff,0xff};	
	for(i=1;8;1){
		var ret = LINDevMng.Transmit3C(data);
		if(#ret > 7){
			var baseline = (ret[4]<<8) + ret[5];
			var signal = (ret[6]<<8) + ret[7];
			thrdTable.rKeyData[i] = {"baseline"=baseline;"signal"=signal};
		}
		data[5] += 1;
		//sleep(1000);
	}
}
